<?xml version="1.0" encoding="UTF-8"?>
<configuration debug="true" scan="false">

	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{HH:mm:ss} [%thread] %-5level %logger{30} - %msg%n
			</pattern>
		</encoder>
	</appender>
	
	<root level="warn">
		<appender-ref ref="STDOUT" />
	</root>
</configuration>